# Container registry & image tag
REGISTRY=registry.hub.docker.com/<your-username>
IMAGE_TAG=v0.1.0

# Kubernetes options
KUBE_NAMESPACE=genai
USE_INGRESS=true
INGRESS_CLASS=nginx
BASE_DOMAIN=your-domain.com

# Runtime defaults
DEFAULT_AGENT_IMAGE=${REGISTRY}/genai-agent-runtime:${IMAGE_TAG}
DEFAULT_LOG_LEVEL=info

# OpenAI (used by agent_runtime if set)
OPENAI_API_KEY=sk-xxx
